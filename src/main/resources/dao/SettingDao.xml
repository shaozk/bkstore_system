<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.SettingDao">



<!--    保存-->
    <insert id="save" >
        insert into `b_settings`(`id`, `key`, `value`, `create_time`, `update_time`)
            value (#{id}, #{key}, #{value}, #{createTime}, #{updateTime})
    </insert>

    <!--    通过key查找-->
    <select id="findOneByKey" resultType="pojo.Setting" >
        select * from `b_settings` where `key` = #{key}
    </select>
</mapper>